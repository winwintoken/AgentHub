    "frontend:dev": "cd app && npm run dev",
    "frontend:build": "cd app && npm run build",
    "frontend:install": "cd app && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git@github-web3study:Web3StudyGroup/0Agent.git"
  },
  "keywords": [
    "blockchain",
    "ai",
    "nft",
    "0g",
    "ethereum"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "hardhat": "^2.26.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "dotenv": "^17.2.2",
    "ethers": "^6.15.0"
  }
}
