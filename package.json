{
  "name": "agenthub",
  "version": "1.0.0",
  "description": "AI agent project using 0G ecosystem",
  "type": "module",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "deploy": "npx hardhat run scripts/deploy.cjs",
    "deploy:localhost": "npx hardhat run scripts/deploy.cjs --network localhost",
